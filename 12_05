   "source": [
    "def isalpha_2(s):\n",
    "   if not s[0].isalpha(): s=s[1:]\n",
    "   if not s[-1].isalpha(): s=s[0:-1]\n",
    "   return s\n",
    "def clear_2(text):\n",
    "    t = []\n",
    "    for word in text.split():\n",
    "        temp = isalpha(word)\n",
    "        if temp.isalpha():\n",
    "            t.append(temp\n",
    "    return t"
   ]
