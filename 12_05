   "source": [
    "def n_grams(word, n):\n",
    "    grams = []\n",
    "    for i in range(0, len(word)-n+1):\n",
    "        grams.append(word[i:i+n])\n",
    "    return grams\n",
    "\n",
    "def generator(text, n):\n",
    "    t = []\n",
    "    for word in clear(text):\n",
    "        t.extend(n_grams(word,n))\n",
    "    return t\n",
    "def find_words(txt_list, ngram):\n",
    "    t = []\n",
    "    for word in txt_list:\n",
    "        if word not in t and ngram in word:\n",
    "            t.append(word)\n",
    "    return t\n",
    "    \n",
    "        \n",
    "def results(txt_list, ngram_list):\n",
    "    t = []\n",
    "    for ng in ngram_list:\n",
    "        temp = (ng,find_words(txt_list, ng))\n",
    "        if temp not in t:\n",
    "            t.append(temp)\n",
    "    return t\n",
    "        \n"
   ]
