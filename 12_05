    "import string\n",
    "import teksty as txt\n",
    "def isalpha(l):\n",
    "    return [ word.lower() for word in l if word.isalpha() ]\n",
    "def clear(text):\n",
    "\n",
    "    t = [ word.strip(string.punctuation+string.digits+' ')\n",
    "         for word in text.split() ]\n",
    "    return isalpha(t)"
